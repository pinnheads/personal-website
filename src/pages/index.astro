---
import BaseLayout from "../layouts/BaseLayout.astro";
import { Image } from "astro:assets";
import SkillsDark from "../assets/skills-bg-dark.png";
import SkillsLight from "../assets/skills-bg-light.png";
---

<BaseLayout>
    <Image
        src={SkillsLight}
        alt="All the technologies I have worked with"
        class="dark:hidden absolute bottom-0 right-0 z-0 animate-fade animate-ease-in animate-duration-[1500ms]"
    />
    <Image
        src={SkillsDark}
        alt="All the technologies I have worked with"
        class="dark:block hidden absolute bottom-0 right-0 z-0 animate-fade animate-ease-in animate-duration-[1500ms]"
    />
</BaseLayout>
